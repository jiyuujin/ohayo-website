{
  "name": "vue-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,vue,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .prettierignore",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "eslint . --ext js,jsx,ts,tsx,vue --ignore-path .gitignore",
    "eslint:fix": "eslint . --ext js,jsx,ts,tsx,vue --ignore-path .gitignore --fix",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.0.20",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-links": "7.0.20",
    "@storybook/vue3": "7.0.20",
    "@storybook/vue3-vite": "7.0.20",
    "@vitejs/plugin-vue": "4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.20",
    "vite": "4.2.2",
    "vite-plugin-inspect": "0.7.28",
    "vite-svg-loader": "4.0.0",
    "vue-loader": "16.8.3"
  }
}
