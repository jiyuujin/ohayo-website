{
  "name": "vite-vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ssg": "vite-ssg build",
    "build:og-gen": "ts-node --project tsconfig.builder.json scripts/og-gen.ts",
    "lint": "eslint ./ --ext ts,vue",
    "lint:fix": "eslint ./ --ext ts,vue --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.8",
    "@nekohack/normalize.css": "^2.4.1",
    "@nekohack/profile-ui": "jiyuujin/profile-ui.git#92c8c720465ab35da150d065ff310b7fa1cd8254",
    "@vue/apollo-composable": "^4.0.0-alpha.14",
    "@vueuse/core": "^6.7.3",
    "@vueuse/head": "^0.6.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "dayjs": "^1.10.7",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "markdown-it": "^12.2.0",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-video": "^0.6.3",
    "ts-node": "^10.4.0",
    "vue": "^3.1.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@nekohack/eslint-config-vue": "^1.1.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/server-renderer": "^3.2.20",
    "@vue/test-utils": "^2.0.0-beta.6",
    "@vueuse/head": "^0.6.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.4.2",
    "puppeteer": "^10.4.0",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7",
    "unplugin-auto-import": "^0.4.13",
    "vite": "^2.3.7",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-ssg": "^0.15.5",
    "vite-svg-loader": "^2.2.0",
    "vue-jest": "^5.0.0-alpha.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "vue",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/tests/*.spec.ts?(x)"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ]
  }
}
